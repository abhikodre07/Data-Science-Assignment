# In[15]:


from sklearn.cluster import AgglomerativeClustering


# In[16]:


ag=AgglomerativeClustering(n_clusters=4)
y=ag.fit_predict(x)


# In[17]:


silhouette_score(x,y)


# In[18]:


df['cluster']=y


# In[19]:


df.head()


# In[21]:


x=pd.DataFrame(x)


